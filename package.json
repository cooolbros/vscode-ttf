{
    "name": "vscode-ttf",
    "displayName": "VSCode TTF",
    "description": "TrueType font viewer for VSCode",
    "version": "1.0.0",
    "license": "MIT",
    "publisher": "pfwobcke",
    "categories": [
        "Visualization"
    ],
    "keywords": [
        "ttf"
    ],
    "icon": "icon.png",
    "main": "./dist/extension.js",
    "browser": "./dist/extension.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/cooolbros/vscode-ttf"
    },
    "engines": {
        "vscode": "^1.98.0"
    },
    "contributes": {
        "customEditors": [
            {
                "viewType": "vscode-ttf.TTFEditor",
                "displayName": "TTF Editor",
                "selector": [
                    {
                        "filenamePattern": "*.ttf"
                    }
                ]
            }
        ]
    },
    "scripts": {
        "dev": "webpack --watch --mode none",
        "build": "webpack",
        "package": "vsce package --no-dependencies",
        "vscode:prepublish": "pnpm build"
    },
    "devDependencies": {
        "@types/vscode": "^1.98.0",
        "@vscode/vsce": "^3.6.0",
        "dedent": "^1.7.0",
        "svelte": "^5.38.10",
        "svelte-loader": "^3.2.4",
        "ts-loader": "^9.5.4",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.2",
        "webpack": "^5.101.3",
        "webpack-cli": "^6.0.1"
    },
    "vsce": {
        "dependencies": false
    },
    "packageManager": "pnpm@10.6.3"
}
